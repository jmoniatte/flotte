# Flotte Configuration
# Copy to ~/.config/flotte/config.toml

# Color theme: onedark or onelight
theme = "onedark"

# Projects (at least one required)
[[projects]]
name = "my-project"
path = "/path/to/my-project"
ride_command = ""

# Add more projects:
# [[projects]]
# name = "another-project"
# path = "/path/to/another"
# ride_command = "my-workspace-script"
